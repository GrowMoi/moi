### moi
server {
  listen 80;
  server_name moi-demo-backend.shiriculapo.com www.moi-demo-backend.shiriculapo.com;
  root /home/moi/moi-backend/current/public;

  passenger_enabled on;
  passenger_app_env demo;
  passenger_ruby /home/moi/.rbenv/versions/2.1.4/bin/ruby;

  location ^~ /assets/ {
    gzip_static on;
    expires max;
    add_header Cache-Control public;
  }

  error_page 500 502 503 504 /500.html;
  keepalive_timeout 10;
  client_max_body_size 4G;
}
